Tab4Section:NewButton("REMOVE GALAXY PORTAL", "remove galaxy portal when he died error", function()

     while wait() do

    game.Workspace.ErrorPortal:Destroy()

    game.Workspace.Trash.Gans.Strings:Destroy()

    game.Workspace.Trash.Galaxy.Strings:Destroy()

    game.Workspace.Trash.Dio.Strings:Destroy()

    game.Workspace.Trash.Error.Strings:Destroy()

end

end)

Tab4Section:NewButton("REMOVE  FOG", "remove any fog", function()

wait()

game.Lighting.FogStart = 10

game.Lighting.FogEnd = 4000000000

game.Lightng.FogColor = Color3.new (0,0,0)

end)
