local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()

local Window = Library.CreateLib("Survive the error", "Ocean")

-- Tabs

local Tab1 = Window:NewTab("main")

local Tab1Section = Tab1:NewSection("main")

local Tab2 = Window:NewTab("blood bath")

local Tab2Section = Tab2:NewSection("blood bath")

local Tab3 = Window:NewTab("Attacks remover")

local Tab3Section = Tab3:NewSection("Attacks remover")

local Tab4 = Window:NewTab("Boss effects remover")

local Tab4Section = Tab4:NewSection("Boss effects remover")

-- Buttons/Windows/Idk

Tab1Section:NewButton("Inf Jumps", "Enables Inf Jumps", function()

    local InfiniteJumpEnabled = true

game:GetService("UserInputService").JumpRequest:connect(function()

	if InfiniteJumpEnabled then		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")

	end

end)

end)

Tab1Section:NewToggle("Fov", "Changes Fov", function(state)

    if state then

        game.Workspace.CurrentCamera.FieldOfView = 120

    else

        game.Workspace.CurrentCamera.FieldOfView = 80

    end

end)

Tab1Section:NewSlider("Speed", "SUSSY speed", 250, 125, function(v)

    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v

end)

Tab2Section:NewButton("KILL NIGHTMARE", "kill nightmare", function()

    game.Workspace.Nightmare.Head:Destroy()

    end)

Tab2Section:NewButton("KILL ENRAGED  NIGHTMARE", "kill enraged nightmare", function()

    game.Workspace.Enraged.Head:Destroy()

    end)

Tab2Section:NewButton("KILL DIO", "kill dio", function()

    game.Workspace.Dio.Head:Destroy()

    end)

Tab2Section:NewButton("KILL GANS", "kill gans", function()

    game.Workspace.Gans.Head:Destroy()

    end)

Tab2Section:NewButton("KILL ERROR", "kill final boss in the game", function()

    game.Workspace.Error.Head:Destroy()

    end)

Tab1Section:NewButton("Delete main door", "This delete closed door when game started", function()

    game.Workspace.Block:Destroy()

    end)

Tab3Section:NewButton("REMOVE DIO ATTACKS", "remove dio attacks", function()

     while wait() do

    game.Workspace.KnifeThrow:Destroy()

    game.Workspace.RoadRollerDa:Destroy()

    game.Workspace.DioBone:Destroy()

    game.Workspace.Zawarudo:Destroy()

    game.Workspace.BuffedZaWarudo:Destroy()

    game.Workspace.ZaWarudo2:Destroy()

    game.Workspace.Punch:destroy()

    end

    end)

Tab3Section:NewButton("REMOVE GANS ATTACKS", "remove gans attacks", function()

     while wait() do

    game.Workspace.HandBlast:Destroy()

    game.Workspace.BoneZone:Destroy()

    game.Workspace.GansBlaster:Destroy()

    game.Workspace.GansBonesUnbuffed:destroy()

    game.Workspace.GansBone:Destroy()

    game.Workspace.GoopGans:Destroy()

    game.Workspace.BuffedGansBlaster:Destroy()

    game.Workspace.UnbuffedGansBlaster:Destroy()

    end

    end)

Tab3Section:NewButton("REMOVE GALAXY ATTACKS", "remove galaxy attacks", function()

     while wait() do

    game.Workspace.GreenStarThrow:Destroy()

    game.Workspace.StarThrow:Destroy()

    game.Workspace.Meteorite:Destroy()

    game.Workspace.StarBlaster:Destroy()

    game.Workspace.ZaWarudo:Destroy()

    game.Workspace.Supernova:Destroy()

    end

    end)

Tab3Section:NewButton("REMOVE ERROR ATTACKS", "remove final boss attacks", function()

